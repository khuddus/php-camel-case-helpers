{
    "name": "khuddus/php-camel-case-helpers",
    "type": "library",
    "description": "A php tool to convert underscored or dashed arrays to camelcased arrays",
    "keywords": [
        "khuddus",
        "camel case json php"
    ],
    "homepage": "https://github.com/khuddus/php-camel-case-helpers",
    "license": "MIT",
    "authors": [
        {
            "name": "Abdul Khuddus",
            "email": "khuddus1+github@gmail.com",
            "homepage": "http://abdulkhuddus.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=7.2.0"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=8.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Khuddus\\Helpers\\CamelCase\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Khuddus\\Helpers\\CamelCase\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
